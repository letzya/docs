{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "modeToggle": {
    "default": "system",
    "dark": false
  },
  "name": "Mint Starter Kit",
  "colors": {
    "primary": "#1e22db",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Home",
        "href": "https://tyk.io"
      },
      {
        "tab": "Guides",
        "groups": [
         {
            "group": "Overview",
            "pages": [
              "/content/api-management/overview/tyk-overview",
              "/content/api-management/overview/tyk-components",
              "/content/api-management/overview/apim"
            ]
          },
          {
            "group": "Getting started",
            "pages": [
              "/content/api-management/getting-started/create-account",
              "/content/api-management/getting-started/configure-first-api",
              "/content/api-management/getting-started/dashboard-config",
              "/content/api-management/getting-started/using-tyk-dashboard"
            ]
          },
          {
            "group": "Deploy Tyk",
            "pages": [
              "/content/api-management/deploy-tyk/tyk-stack",
              "/content/api-management/deploy-tyk/tyk-cloud",
              "/content/api-management/deploy-tyk/tyk-open-source",
              "/content/api-management/deploy-tyk/tyk-self-managed"
            ]
          }
        ]
      },
      {
        "tab": "Developer Portal",
        "groups": [
          {
            "group": "empty",
            "pages": [
              "/content/developer-portal/overview",
              "/content/developer-portal/settings",
              "/content/developer-portal/customization"
            ]
          }
        ]
      },
      {
        "tab": "AI Management",
        "groups": [
          {
            "group": "AI Studio",
            "pages": [
              "/content/ai-management/ai-studio/overview"
            ]
          },
          {
            "group": "MCPs",
            "pages": [
              "/content/ai-management/mcps/overview",
              "/content/ai-management/mcps/api-to-mcp",
              "/content/ai-management/mcps/dashboard-api-to-mcp"
            ]
          }
        ]
      },
      {
        "tab": "API Governance",
        "groups": [
          {
            "group": "empty",
            "pages": [
              "/content/developer-portal/overview/tyk-overview",
              "/content/developer-portal/overview/tyk-components",
              "/content/developer-portal/overview/apim"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://mintlify.com/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Community",
          "href": "https://mintlify.com/community",
          "icon": "slack"
        },
        {
          "anchor": "Blog",
          "href": "https://mintlify.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "https://tyk.io/docs/img/tyk-logo.png",
    "dark": "https://tyk.io/docs/img/Tyk-Docs-logo-Dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "https://tyk.io/contact/"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard-tyk",
      "href": "https://dashboard.mintlify.com"
    }
  },
  "footer": {
    "socials": {
      "x": "https://twitter.com/tyk_io",
      "github": "https://github.com/TykTechnologies/tyk",
      "linkedin": "https://www.linkedin.com/company/tyk/"
    }
  }
}
